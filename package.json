{
  "name": "the-good-code-server",
  "version": "0.0.1",
  "description": "NodeJS CRUD Server",
  "main": "index.ts",
  "author": "Damian Kmiecik",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "format": "prettier --write *.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "concurrently": "^7.2.2",
    "husky": "^8.0.1",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
